+++ b/eclipse-plugins/edu.wpi.first.wpilib.plugins.cpp/src/main/resources/cpp-zip/ant/build.properties
@@ -1,5 +1,5 @@
 # Deployment information
-username=admin
+username=lvuser
 password=
 deploy.dir=/home/lvuser
 deploy.kill.command=/usr/local/frc/bin/frcKillRobot.sh -t -r
