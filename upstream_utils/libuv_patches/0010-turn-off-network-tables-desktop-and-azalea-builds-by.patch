From: brad <brad@bradhouse.com>
Date: Thu, 23 Jan 2014 18:13:20 -0500
Subject: [PATCH 0010/6262] turn off network tables desktop and azalea builds
 by default

---
 networktables/cpp/pom.xml | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/networktables/cpp/pom.xml b/networktables/cpp/pom.xml
index ccac2f6eab1d7f2b8f414ad37fb76eb4fa5c74a4..238bf5ca4dfb3aad54345f1e4d88d8015f7de43d 100644
--- a/networktables/cpp/pom.xml
+++ b/networktables/cpp/pom.xml
@@ -27,7 +27,7 @@
 	<profile>
 	  <id>desktop</id>
       <activation>
-		<activeByDefault>true</activeByDefault>
+		<activeByDefault>false</activeByDefault>
       </activation>
 	  <modules>
 		<module>parent</module>
@@ -46,7 +46,7 @@
 	<profile>
 	  <id>azalea</id>
       <activation>
-		<activeByDefault>true</activeByDefault>
+		<activeByDefault>false</activeByDefault>
       </activation>
 	  <modules>
 		<module>Azalea</module>
